<%include header%>
<div class="container">
    <form class="form-horizatal" method="post" action="">
        <h2>编辑博客</h2>
        <table class="table table-hover">
            <tr>
                <td>标题</td>
                <td><input type="text" name="title" value="<%= post.title%>"></td>
            </tr>
            <tr>
                <td>内容</td>
                <td><textarea  name="content"> <%= post.content%></textarea></td>
            </tr>
            <tr>
                <td>标签</td>
                <td><input name="tags" value="<%= post.tags%>"></td>
            </tr>
            <tr>
            <td>
                <button class="btn btn-primary"> 编辑</button>
            </td>
            </tr>
        </table>
    </form>
</div>
<%include footer%>